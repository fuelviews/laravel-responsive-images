{
    "name": "fuelviews/laravel-responsiveimages",
    "description": "Post-processed responsive images for Laravel.",
    "type": "library",
    "keywords": ["laravel", "responsive images"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/fuelviews/laravel-responsiveimages/issues",
        "source": "https://github.com/fuelviews/laravel-responsiveimages"
    },
    "authors": [
        {
            "name": "Joshua Mitchener",
            "email": "support@fuelviews.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0",
        "illuminate/view": "^10.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Fuelviews\\ResponsiveImages\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.x-dev"
        },
        "laravel": {
            "providers": [
                "Fuelviews\\ResponsiveImages\\ResponsiveImagesServiceProvider"
            ],
            "aliases": {
                "ResponsiveImages": "Fuelviews\\ResponsiveImages"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
